<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.concussionconnect.concussionproject" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChecklistActivity"></activity>
        <activity android:name=".WordMemoryActivity"></activity>
        <activity android:name=".EyeTrackingActivity"></activity>
        <activity android:name=".BalanceActivity"></activity>
        <activity android:name=".RateListActivity"></activity>
        <activity android:name=".Page2"></activity>
        <activity android:name=".Page3"></activity>
        <activity android:name=".Page4"></activity>
        <activity android:name=".Page5"></activity>
        <activity android:name=".Page6"></activity>

        <activity android:name=".Result"></activity>

    </application>
</manifest>
