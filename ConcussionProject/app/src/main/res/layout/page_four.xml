<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:layout_gravity="center"
     tools:context=".Page4">

    <ProgressBar
        android:id="@+id/progressBar1"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:indeterminate="false"
        android:max="5"
        android:minHeight="50dp"
        android:layout_gravity="center"
        android:minWidth="200dp"
        android:progress="3"
        android:layout_centerHorizontal="true" />
    <LinearLayout
        android:id="@+id/list1"
        android:layout_below="@+id/progressBar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical">
        <TextView
            android:id="@+id/label1"
            android:text="@string/symptom11"
            android:paddingLeft="1px"
            android:paddingRight="1px"
            android:paddingTop="1px"
            android:textSize="16sp"
            android:layout_above="@+id/rate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <RatingBar
            style="@style/StarRatingBar"
            android:id="@+id/rate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:stepSize="1"
            android:numStars="6"
            android:layout_centerHorizontal="true"
            />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/list2"
        android:layout_below="@+id/list1"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical">
        <TextView
            android:id="@+id/label2"
            android:text="@string/symptom12"
            android:paddingLeft="1px"
            android:paddingRight="1px"
            android:paddingTop="1px"
            android:textSize="16sp"
            android:layout_above="@+id/rate2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <RatingBar
            style="@style/StarRatingBar"
            android:id="@+id/rate2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:numStars="6"
            android:stepSize="1"

            android:layout_centerHorizontal="true"
            />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/list3"
        android:layout_below="@+id/list2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical">
        <TextView
            android:id="@+id/label3"
            android:text="@string/symptom13"
            android:paddingLeft="1px"
            android:paddingRight="1px"
            android:paddingTop="1px"
            android:textSize="16sp"
            android:layout_above="@+id/rate3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <RatingBar
            style="@style/StarRatingBar"
            android:id="@+id/rate3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:numStars="6"
            android:stepSize="1"

            android:layout_centerHorizontal="true"
            />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/list4"
        android:layout_below="@+id/list3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical">
        <TextView
            android:id="@+id/label4"
            android:text="@string/symptom14"
            android:paddingLeft="1px"
            android:paddingRight="1px"
            android:paddingTop="1px"
            android:textSize="16sp"
            android:layout_above="@+id/rate3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <RatingBar
            style="@style/StarRatingBar"
            android:id="@+id/rate4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:numStars="6"
            android:stepSize="1"

            android:layout_centerHorizontal="true"
            />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/list5"
        android:layout_below="@+id/list4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical">
        <TextView
            android:id="@+id/label5"
            android:text="@string/symptom15"
            android:paddingLeft="1px"
            android:paddingRight="1px"
            android:paddingTop="1px"
            android:textSize="16sp"
            android:layout_above="@+id/rate3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <RatingBar
            style="@style/StarRatingBar"
            android:id="@+id/rate5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:numStars="6"
            android:stepSize="1"
            android:layout_centerHorizontal="true"
            />
    </LinearLayout>

        <Button
            android:layout_width= "fill_parent"
            android:id="@+id/but"
            android:layout_height= "wrap_content"
            android:layout_gravity="bottom"
            android:text = "Next"
            android:onClick="next"
            android:layout_alignParentBottom="true" />



</RelativeLayout>
